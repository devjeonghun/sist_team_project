package com.aven.model;

public class PaymentBean {
	private int p_no;
	private int hrr_no;
	private String p_type;
	private String p_data;
	private String p_cor;

	public int getP_no() {
		return p_no;
	}

	public void setP_no(int p_no) {
		this.p_no = p_no;
	}

	public int getHrr_no() {
		return hrr_no;
	}

	public void setHrr_no(int hrr_no) {
		this.hrr_no = hrr_no;
	}

	public String getP_type() {
		return p_type;
	}

	public void setP_type(String p_type) {
		this.p_type = p_type;
	}

	public String getP_data() {
		return p_data;
	}

	public void setP_data(String p_data) {
		this.p_data = p_data;
	}

	public String getP_cor() {
		return p_cor;
	}

	public void setP_cor(String p_cor) {
		this.p_cor = p_cor;
	}

	@Override
	public String toString() {
		return "PaymentBean [p_no=" + p_no + ", hrr_no=" + hrr_no + ", p_type=" + p_type + ", p_data=" + p_data
				+ ", p_cor=" + p_cor + "]";
	}

}
